{
  "name": "suomifi-design-system",
  "private": true,
  "description": "Suomi.fi Design System",
  "version": "0.1.0",
  "author": "Jari Porola <jari.porola@gmail.com>",
  "dependencies": {
    "@csstools/normalize.css": "^9.0.1",
    "@emotion/core": "^10.0.6",
    "@wapps/gatsby-plugin-i18next": "^1.1.3",
    "gatsby": "^2.0.91",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-emotion": "^4.0.3",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-typescript": "^2.0.8",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-code": "^0.1.0",
    "gatsby-transformer-sharp": "^2.1.8",
    "i18next": "^15.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-element-to-jsx-string": "^14.0.2",
    "react-helmet": "^5.2.0",
    "react-i18next": "8",
    "react-motion": "^0.5.2",
    "react-responsive": "^6.1.1",
    "react-syntax-highlighter": "^10.1.2",
    "suomifi-ui-components": "^0.0.12"
  },
  "resolutions": {
    "@wapps/gatsby-plugin-i18next/i18next": "^15.0.6"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .cache public",
    "build": "yarn clean && gatsby build",
    "develop": "yarn clean && gatsby develop",
    "start": "yarn develop",
    "lint:code": "eslint \"src/**/*.{js,jsx,ts,tsx,md}\"",
    "format:code": "prettier-eslint --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:style": "stylelint \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format:style": "prettier-stylelint --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "validate": "yarn run lint:code && yarn run lint:style && yarn run test",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "yarn build --prefix-paths && gh-pages -d public -m \"[skip ci] Updates\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn validate"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md}": [
      "eslint"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,scss}": [
      "stylelint"
    ]
  },
  "devDependencies": {
    "@types/i18next": "^12.1.0",
    "@types/react": "^16.7.19",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/react-i18next": "^8.1.0",
    "@types/react-responsive": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.1",
    "prettier": "^1.15.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.3",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "github:vrk-kpa/suomifi-design-system"
  },
  "bugs": {
    "url": "https://github.com/vrk-kpa/suomifi-design-system/issues"
  }
}
